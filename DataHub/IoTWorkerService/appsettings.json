{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SqlWorkerOptions": {
    "Host": "host_where_sql_database_is_running",
    "Database": "database_name",
    "UserName": "database_user_name",
    "Password": "database_password"
  },
  "MqttOptions": {
    "Uri": "your_mqtt_uri",
    "Port": "mqtt_port",
    "SubscriptionTopic": "your_topic"
  }
}
